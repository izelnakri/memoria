{
  "name": "memserver",
  "version": "1.9.0",
  "description": "in-memory database/ORM and http mock server you can run in-browser and node environments. Built for large frontend teams, fast tests and rapid prototyping",
  "author": "Izel Nakri",
  "repository": {
    "type": "git",
    "url": "https://github.com/izelnakri/memserver.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/memserver"
  },
  "license": "MIT",
  "bin": {
    "memserver": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node_modules/.bin/ava --verbose",
    "bin": "node cli.js"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "ember-cli-string-utils": "^1.1.0",
    "ember-source": "^3.16.0",
    "fake-xml-http-request": "^2.1.1",
    "fs-extra": "^8.1.0",
    "i": "^0.3.6",
    "qs": "^6.9.1",
    "route-recognizer": "^0.3.4"
  },
  "devDependencies": {
    "@ava/typescript": "^1.0.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jquery": "^3.3.31",
    "@types/node": "^13.5.3",
    "ava": "3.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ember-cli-fastboot": "2.2.1",
    "fastboot": "^2.0.3",
    "jquery": "^3.4.1",
    "mber": "^0.9.1",
    "sinon": "^8.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "ava": {
    "files": [
      "src/test/**/*"
    ],
    "extensions": [
      "ts"
    ],
    "cache": false,
    "require": [
      "ts-node/register"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "volta": {
    "node": "13.6.0"
  }
}
